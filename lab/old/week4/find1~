LXI H,0000H
MVI B,00H ; NEGATIVE
MVI C,00H ; POSITIVE
MVI D,00H ; ZERO
MVI E,32H

NXT: 	MOV A,M
	RAL 
	JC NEG
	MOV A,M
	ORI 00H
	JZ ZERO
	INR C
CHECK:	INX H	
	DCR E
	JNZ NXT
	JZ STORE
NEG:	INR B
	JMP CHECK
	
ZERO:	INR D
	JMP CHECK
STORE:	LXI H,9100H
	MOV M,B ;NEGATIVE
	INX H
	MOV M,C ;POSITIVE	
	INX H
	MOV M,D ;ZERO
hlt